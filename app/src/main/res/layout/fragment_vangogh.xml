<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".VangoghFrag">

    <!-- TODO: Update blank fragment layout -->

    <ImageView
        android:id="@+id/vangogh1"
        android:layout_width="204dp"
        android:layout_height="170dp"
        android:src="@drawable/vangogh1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.077"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.413" />

    <ImageView
        android:id="@+id/vangogh2"
        android:layout_width="211dp"
        android:layout_height="184dp"
        android:src="@drawable/vangogh2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vangogh1"
        app:layout_constraintVertical_bias="0.188" />

    <TextView
        android:id="@+id/titlegogh"
        android:layout_width="326dp"
        android:layout_height="84dp"
        android:text="Paintings by Vincent VanGogh"
        android:textColor="@color/ochre"
        android:textSize="48sp"
        app:layout_constraintBottom_toTopOf="@+id/vangogh1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.261" />

    <Button
        android:id="@+id/btnvangogh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/darkpink_trans"
        android:text="download"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vangogh2"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="635dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="126dp"
        android:layout_height="37dp"
        android:background="@color/darkpink_trans"
        android:backgroundTint="@color/darkpink"
        android:lineSpacingExtra="4sp"
        android:text="The Starry Night "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintStart_toEndOf="@+id/vangogh1"
        app:layout_constraintTop_toBottomOf="@+id/titlegogh"
        app:layout_constraintVertical_bias="0.189" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="127dp"
        android:layout_height="48dp"
        android:background="@color/darkpink_trans"
        android:backgroundTint="@color/darkpink"
        android:lineSpacingExtra="4sp"
        android:text="Title: Garden with Courting Couples"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/vangogh2"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.529" />

</androidx.constraintlayout.widget.ConstraintLayout>